<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core"
      xmlns:fbc="http://www.sbml.org/sbml/level3/version1/fbc/version2"
      level="3" version="1" fbc:required="true">
  <model id="LU" name="LU">
    <listOfFunctionDefinitions/>
    <listOfUnitDefinitions/>
    <listOfCompartments>
      <compartment id="e" name="extracellular" constant="true"/>
      <compartment id="c" name="cytosol" constant="true"/>
    </listOfCompartments>
    <listOfParameters/>
    <listOfSpecies>
      <species id="lac__L_e" name="L-Lactate [e]" compartment="e" initialAmount="0" boundaryCondition="true" hasOnlySubstanceUnits="false" constant="false"/>
      <species id="lac__L_c" name="L-Lactate [c]" compartment="c" initialAmount="0" boundaryCondition="false" hasOnlySubstanceUnits="false" constant="false"/>
      <species id="but_e" name="Butyrate [e]" compartment="e" initialAmount="0" boundaryCondition="true" hasOnlySubstanceUnits="false" constant="false"/>
      <species id="but_c" name="Butyrate [c]" compartment="c" initialAmount="0" boundaryCondition="false" hasOnlySubstanceUnits="false" constant="false"/>
      <species id="biomass_c" name="Biomass [c]" compartment="c" initialAmount="0" boundaryCondition="false" hasOnlySubstanceUnits="false" constant="false"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="EX_lac__L_e" name="Exchange L-Lactate (e)" reversible="true">
        <fbc:fluxBounds>
          <fbc:fluxBound fbc:operation="greaterEqual" fbc:reaction="EX_lac__L_e" fbc:value="-1000"/>
          <fbc:fluxBound fbc:operation="lessEqual" fbc:reaction="EX_lac__L_e" fbc:value="1000"/>
        </fbc:fluxBounds>
        <listOfReactants>
          <speciesReference species="lac__L_e" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts/>
      </reaction>
      <reaction id="EX_but_e" name="Exchange Butyrate (e)" reversible="true">
        <fbc:fluxBounds>
          <fbc:fluxBound fbc:operation="greaterEqual" fbc:reaction="EX_but_e" fbc:value="-1000"/>
          <fbc:fluxBound fbc:operation="lessEqual" fbc:reaction="EX_but_e" fbc:value="1000"/>
        </fbc:fluxBounds>
        <listOfReactants>
          <speciesReference species="but_e" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts/>
      </reaction>
      <reaction id="T_lac" name="Transport lactate e->c" reversible="false">
        <fbc:fluxBounds>
          <fbc:fluxBound fbc:operation="greaterEqual" fbc:reaction="T_lac" fbc:value="0"/>
          <fbc:fluxBound fbc:operation="lessEqual" fbc:reaction="T_lac" fbc:value="1000"/>
        </fbc:fluxBounds>
        <listOfReactants>
          <speciesReference species="lac__L_e" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lac__L_c" stoichiometry="1"/>
        </listOfProducts>
      </reaction>
      <reaction id="T_but" name="Transport butyrate c->e" reversible="false">
        <fbc:fluxBounds>
          <fbc:fluxBound fbc:operation="greaterEqual" fbc:reaction="T_but" fbc:value="0"/>
          <fbc:fluxBound fbc:operation="lessEqual" fbc:reaction="T_but" fbc:value="1000"/>
        </fbc:fluxBounds>
        <listOfReactants>
          <speciesReference species="but_c" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="but_e" stoichiometry="1"/>
        </listOfProducts>
      </reaction>
      <reaction id="BUT" name="Lactate to butyrate" reversible="false">
        <fbc:fluxBounds>
          <fbc:fluxBound fbc:operation="greaterEqual" fbc:reaction="BUT" fbc:value="0"/>
          <fbc:fluxBound fbc:operation="lessEqual" fbc:reaction="BUT" fbc:value="1000"/>
        </fbc:fluxBounds>
        <listOfReactants>
          <speciesReference species="lac__L_c" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="but_c" stoichiometry="0.9"/>
        </listOfProducts>
      </reaction>
      <reaction id="BIOMASS_LU" name="Biomass drain LU" reversible="false">
        <fbc:fluxBounds>
          <fbc:fluxBound fbc:operation="greaterEqual" fbc:reaction="BIOMASS_LU" fbc:value="0"/>
          <fbc:fluxBound fbc:operation="lessEqual" fbc:reaction="BIOMASS_LU" fbc:value="1000"/>
        </fbc:fluxBounds>
        <listOfReactants>
          <speciesReference species="lac__L_c" stoichiometry="0.01"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="biomass_c" stoichiometry="1"/>
        </listOfProducts>
      </reaction>
    </listOfReactions>
<fbc:listOfObjectives fbc:activeObjective="obj">
  <fbc:objective fbc:id="obj" fbc:type="maximize">
    <fbc:listOfFluxObjectives>
      <fbc:fluxObjective fbc:reaction="BIOMASS_LU" fbc:coefficient="1.0"/>
    </fbc:listOfFluxObjectives>
  </fbc:objective>
</fbc:listOfObjectives>
  </model>
</sbml>
