<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core"
      xmlns:fbc="http://www.sbml.org/sbml/level3/version1/fbc/version2"
      level="3" version="1" fbc:required="true" fbc:strict="true">
  <model id="BP" name="BP">
    <listOfCompartments>
      <compartment id="c" name="cytosol" constant="true" size="1"/>
      <compartment id="e" name="extracellular" constant="true" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="ac_e" name="Acetate (e)" compartment="e" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="but_e" name="Butyrate (e)" compartment="e" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="ac_c" name="Acetate (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="but_c" name="Butyrate (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="biomass_c" name="Biomass (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="LB_EX_ac_e" value="-10" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_EX_ac_e" value="0" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_EX_but_e" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_EX_but_e" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_T_ac" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_T_ac" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_T_but" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_T_but" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_BPF" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_BPF" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_BIOMASS_BP" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_BIOMASS_BP" value="1000" constant="true" fbc:type="upperFluxBound"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="EX_ac_e" name="Exchange Acetate (e)" reversible="false" fbc:lowerFluxBound="LB_EX_ac_e" fbc:upperFluxBound="UB_EX_ac_e">
        <listOfReactants>
          <speciesReference species="ac_e" stoichiometry="1"/>
        </listOfReactants>
      </reaction>

      <reaction id="EX_but_e" name="Exchange Butyrate (e)" reversible="false" fbc:lowerFluxBound="LB_EX_but_e" fbc:upperFluxBound="UB_EX_but_e">
        <listOfReactants>
          <speciesReference species="but_e" stoichiometry="1"/>
        </listOfReactants>
      </reaction>

      <reaction id="T_ac" name="Transport acetate e->c" reversible="false" fbc:lowerFluxBound="LB_T_ac" fbc:upperFluxBound="UB_T_ac">
        <listOfReactants>
          <speciesReference species="ac_e" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="ac_c" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="T_but" name="Transport butyrate c->e" reversible="false" fbc:lowerFluxBound="LB_T_but" fbc:upperFluxBound="UB_T_but">
        <listOfReactants>
          <speciesReference species="but_c" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="but_e" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="BPF" name="Acetate to butyrate" reversible="false" fbc:lowerFluxBound="LB_BPF" fbc:upperFluxBound="UB_BPF">
        <listOfReactants>
          <speciesReference species="ac_c" stoichiometry="2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="but_c" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="BIOMASS_BP" name="Biomass drain BP" reversible="false" fbc:lowerFluxBound="LB_BIOMASS_BP" fbc:upperFluxBound="UB_BIOMASS_BP">
        <listOfReactants>
          <speciesReference species="ac_c" stoichiometry="0.05"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="biomass_c" stoichiometry="0.05"/>
        </listOfProducts>
      </reaction>

    </listOfReactions>
  </model>
</sbml>
