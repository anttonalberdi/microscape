<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core"
      xmlns:fbc="http://www.sbml.org/sbml/level3/version1/fbc/version2"
      level="3" version="1" fbc:required="true" fbc:strict="true">
  <model id="LU" name="LU">
    <listOfCompartments>
      <compartment id="c" name="cytosol" constant="true" size="1"/>
      <compartment id="e" name="extracellular" constant="true" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="lac__L_e" name="L-Lactate (e)" compartment="e" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="but_e" name="Butyrate (e)" compartment="e" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="lac__L_c" name="L-Lactate (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="but_c" name="Butyrate (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="biomass_c" name="Biomass (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="LB_EX_lac__L_e" value="-10" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_EX_lac__L_e" value="0" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_EX_but_e" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_EX_but_e" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_T_lac" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_T_lac" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_T_but" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_T_but" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_BUT" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_BUT" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_BIOMASS_LU" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_BIOMASS_LU" value="1000" constant="true" fbc:type="upperFluxBound"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="EX_lac__L_e" name="Exchange L-Lactate (e)" reversible="false" fbc:lowerFluxBound="LB_EX_lac__L_e" fbc:upperFluxBound="UB_EX_lac__L_e">
        <listOfReactants>
          <speciesReference species="lac__L_e" stoichiometry="1"/>
        </listOfReactants>
      </reaction>

      <reaction id="EX_but_e" name="Exchange Butyrate (e)" reversible="false" fbc:lowerFluxBound="LB_EX_but_e" fbc:upperFluxBound="UB_EX_but_e">
        <listOfReactants>
          <speciesReference species="but_e" stoichiometry="1"/>
        </listOfReactants>
      </reaction>

      <reaction id="T_lac" name="Transport lactate e->c" reversible="false" fbc:lowerFluxBound="LB_T_lac" fbc:upperFluxBound="UB_T_lac">
        <listOfReactants>
          <speciesReference species="lac__L_e" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lac__L_c" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="T_but" name="Transport butyrate c->e" reversible="false" fbc:lowerFluxBound="LB_T_but" fbc:upperFluxBound="UB_T_but">
        <listOfReactants>
          <speciesReference species="but_c" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="but_e" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="BUT" name="Lactate to butyrate" reversible="false" fbc:lowerFluxBound="LB_BUT" fbc:upperFluxBound="UB_BUT">
        <listOfReactants>
          <speciesReference species="lac__L_c" stoichiometry="2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="but_c" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="BIOMASS_LU" name="Biomass drain LU" reversible="false" fbc:lowerFluxBound="LB_BIOMASS_LU" fbc:upperFluxBound="UB_BIOMASS_LU">
        <listOfReactants>
          <speciesReference species="lac__L_c" stoichiometry="0.05"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="biomass_c" stoichiometry="0.05"/>
        </listOfProducts>
      </reaction>

    </listOfReactions>
  </model>
</sbml>
