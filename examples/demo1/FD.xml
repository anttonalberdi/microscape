<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core"
      xmlns:fbc="http://www.sbml.org/sbml/level3/version1/fbc/version2"
      level="3" version="1" fbc:required="true" fbc:strict="true">
  <model id="FD" name="FD">
    <listOfCompartments>
      <compartment id="c" name="cytosol" constant="true" size="1"/>
      <compartment id="e" name="extracellular" constant="true" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="glc__D_e" name="D-Glucose (e)" compartment="e" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="lac__L_e" name="L-Lactate (e)" compartment="e" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="ac_e" name="Acetate (e)" compartment="e" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="glc__D_c" name="D-Glucose (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="lac__L_c" name="L-Lactate (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="ac_c" name="Acetate (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="biomass_c" name="Biomass (c)" compartment="c" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="LB_EX_glc__D_e" value="-10" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_EX_glc__D_e" value="0" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_EX_lac__L_e" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_EX_lac__L_e" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_EX_ac_e" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_EX_ac_e" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_T_glc" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_T_glc" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_T_lac" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_T_lac" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_T_ac" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_T_ac" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_FER" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_FER" value="1000" constant="true" fbc:type="upperFluxBound"/>
      <parameter id="LB_BIOMASS_FD" value="0" constant="true" fbc:type="lowerFluxBound"/>
      <parameter id="UB_BIOMASS_FD" value="1000" constant="true" fbc:type="upperFluxBound"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="EX_glc__D_e" name="Exchange D-Glucose (e)" reversible="false" fbc:lowerFluxBound="LB_EX_glc__D_e" fbc:upperFluxBound="UB_EX_glc__D_e">
        <listOfReactants>
          <speciesReference species="glc__D_e" stoichiometry="1"/>
        </listOfReactants>
      </reaction>

      <reaction id="EX_lac__L_e" name="Exchange L-Lactate (e)" reversible="false" fbc:lowerFluxBound="LB_EX_lac__L_e" fbc:upperFluxBound="UB_EX_lac__L_e">
        <listOfReactants>
          <speciesReference species="lac__L_e" stoichiometry="1"/>
        </listOfReactants>
      </reaction>

      <reaction id="EX_ac_e" name="Exchange Acetate (e)" reversible="false" fbc:lowerFluxBound="LB_EX_ac_e" fbc:upperFluxBound="UB_EX_ac_e">
        <listOfReactants>
          <speciesReference species="ac_e" stoichiometry="1"/>
        </listOfReactants>
      </reaction>

      <reaction id="T_glc" name="Transport glucose e->c" reversible="false" fbc:lowerFluxBound="LB_T_glc" fbc:upperFluxBound="UB_T_glc">
        <listOfReactants>
          <speciesReference species="glc__D_e" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="glc__D_c" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="T_lac" name="Transport lactate c->e" reversible="false" fbc:lowerFluxBound="LB_T_lac" fbc:upperFluxBound="UB_T_lac">
        <listOfReactants>
          <speciesReference species="lac__L_c" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lac__L_e" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="T_ac" name="Transport acetate c->e" reversible="false" fbc:lowerFluxBound="LB_T_ac" fbc:upperFluxBound="UB_T_ac">
        <listOfReactants>
          <speciesReference species="ac_c" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="ac_e" stoichiometry="1"/>
        </listOfProducts>
      </reaction>

      <reaction id="FER" name="Glucose fermentation" reversible="false" fbc:lowerFluxBound="LB_FER" fbc:upperFluxBound="UB_FER">
        <listOfReactants>
          <speciesReference species="glc__D_c" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lac__L_c" stoichiometry="1.5"/>
          <speciesReference species="ac_c" stoichiometry="0.5"/>
        </listOfProducts>
      </reaction>

      <reaction id="BIOMASS_FD" name="Biomass drain FD" reversible="false" fbc:lowerFluxBound="LB_BIOMASS_FD" fbc:upperFluxBound="UB_BIOMASS_FD">
        <listOfReactants>
          <speciesReference species="glc__D_c" stoichiometry="0.05"/>
          <speciesReference species="ac_c" stoichiometry="0.05"/>
          <speciesReference species="lac__L_c" stoichiometry="0.05"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="biomass_c" stoichiometry="0.05"/>
        </listOfProducts>
      </reaction>

    </listOfReactions>
  </model>
</sbml>
